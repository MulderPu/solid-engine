<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.mingf.imagenote.ResultActivity"
    android:background="#E0E0E0">


    <TableLayout android:layout_height="match_parent"
        android:layout_width="match_parent"
        >

        <TableRow>
            <TextView
                android:text="@string/name_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewName"
                style="@style/FormCustomStyle"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewName2"
                style="@style/FormCustomStyle"/>
        </TableRow>

        <TableRow>
            <TextView
                android:text="@string/short_description_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewDesc"
                style="@style/FormCustomStyle"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewDesc2"
                style="@style/FormCustomStyle"/>
        </TableRow>

        <TableRow>
            <TextView
                android:text="@string/url_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewUrl"
                style="@style/FormCustomStyle"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewUrl2"
                style="@style/FormCustomStyle"/>

        </TableRow>

        <TableRow>
            <TextView
                android:text="@string/keywords_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewKeywords"
                style="@style/FormCustomStyle"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewKeywords2"
                style="@style/FormCustomStyle"/>
        </TableRow>

        <TableRow>
            <TextView
                android:text="@string/date_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewDate"
                style="@style/FormCustomStyle"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewDate2"
                style="@style/FormCustomStyle"/>
        </TableRow>

        <TableRow>
            <TextView
                android:text="@string/share_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewShare"
                style="@style/FormCustomStyle"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewShare2"
                style="@style/FormCustomStyle"/>

        </TableRow>

        <TableRow>
            <TextView
                android:text="@string/email_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewEmail"
                style="@style/FormCustomStyle"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewEmail2"
                style="@style/FormCustomStyle"/>

        </TableRow>

        <TableRow>
            <TextView
                android:text="@string/rate_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewRate"
                style="@style/FormCustomStyle"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textViewRate2"
                style="@style/FormCustomStyle"/>
        </TableRow>
    </TableLayout>
</LinearLayout>
